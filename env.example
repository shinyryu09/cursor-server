# MCP Cursor Server 환경 변수 설정

# 서버 설정
PORT=3000
HOST=localhost
NODE_ENV=development

# OpenAI 설정
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic 설정
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google 설정
GOOGLE_API_KEY=your_google_api_key_here

# Cursor 설정
CURSOR_API_KEY=your_cursor_api_key_here
CURSOR_BASE_URL=https://api.cursor.com/v1
CURSOR_WORKING_DIR=/Users/username/Projects

# 로깅 설정
LOG_LEVEL=info
LOG_FORMAT=combined

# 프로젝트 검색 경로 (선택사항)
XCODE_SEARCH_PATHS=/Users/username/Documents,/Users/username/Desktop
ANDROID_SEARCH_PATHS=/Users/username/Documents,/Users/username/AndroidStudioProjects
